{
  "name": "template-nodejs-serverless",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.x"
  },
  "scripts": {
    "start": "serverless offline --verbose",
    "format": "prettier \"src/**/*.{js,ts}\" --write",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "lint:fix": "eslint \"src/**/*.{js,ts}\" --fix",
    "test": "TZ=Asia/Tokyo jest --passWithNoTests --collect-coverage --silent=true",
    "test:log": "TZ=Asia/Tokyo jest --passWithNoTests",
    "deploy:develop": "npx sls deploy --stage develop",
    "deploy:production": "npx sls deploy --stage production",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.775.0",
    "@aws-sdk/lib-storage": "^3.832.0",
    "@google-cloud/bigquery": "^8.1.0",
    "@slack/web-api": "^7.9.1",
    "axios": "^1.7.9",
    "axios-retry": "^4.5.0",
    "dayjs": "^1.11.13",
    "googleapis": "^148.0.0",
    "js-base64": "^3.7.7",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@arlequins/common-config": "1.0.0",
    "@arlequins/eslint-config-typescript": "1.0.0",
    "serverless": "^4.6.2",
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-offline": "^14.4.0",
    "serverless-step-functions": "^3.22.0"
  },
  "peerDependencies": {
    "@types/jest": "^29.5.14",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.2.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-sonarjs": "^3.0.2",
    "globals": "^15.14.0",
    "husky": "9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.27.0"
  }
}
