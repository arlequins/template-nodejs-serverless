{
  "name": "template-nodejs-serverless",
  "version": "1.0.0",
  "engines": {
    "node": ">=22.x"
  },
  "scripts": {
    "start": "serverless offline --verbose",
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "test": "TZ=Asia/Tokyo jest --passWithNoTests --collect-coverage --silent=true",
    "test:log": "TZ=Asia/Tokyo jest --passWithNoTests",
    "doc": "node ./docs/swagger/generate-swagger.js",
    "doc:local": "npm run doc && npx --no-install serve ./out",
    "deploy:develop": "serverless deploy --stage develop",
    "deploy:production": "serverless deploy --stage production",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.872.0",
    "@aws-sdk/lib-storage": "^3.872.0",
    "@codegenie/serverless-express": "^4.17.0",
    "@google-cloud/bigquery": "^8.1.0",
    "@slack/web-api": "^7.9.3",
    "axios": "^1.11.0",
    "axios-retry": "^4.5.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "express": "^5.1.0",
    "googleapis": "^156.0.0",
    "js-base64": "^3.7.8",
    "lodash": "^4.17.21",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@arlequins/common-config": "1.0.1",
    "@arlequins/eslint-config-typescript": "1.0.1",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "serverless": "^4.18.0",
    "serverless-add-api-key": "^4.2.1",
    "serverless-associate-waf": "^1.2.1",
    "serverless-deployment-bucket": "^1.6.0",
    "serverless-domain-manager": "^8.0.0",
    "serverless-lift": "^1.31.0",
    "serverless-offline": "^14.4.0",
    "serverless-step-functions": "^3.23.3",
    "swagger-jsdoc": "^6.2.8"
  },
  "peerDependencies": {
    "@types/jest": "^29.5.14",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-import-resolver-typescript": "^4.2.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-sonarjs": "^3.0.2",
    "globals": "^15.14.0",
    "husky": "9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "prettier": "^3.5.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.27.0"
  }
}
